<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  <link rel="stylesheet" href="./css/style.css" >
  </head>
  <body>
    <ul>
      {{#todos}}
      {{#yetTodo}}
    <li>
      <form action"/complete" method="post">
        <button type="submit" name="complete" value="{{todo}}">Mark Complete</button>
        {{todo}}
      </form>
    </li>
    {{/yetTodo}}
    {{/todos}}
    </ul>
    <h1>Completed Things</h1>
    <ul>
      {{#todos}}
      {{^yetTodo}}
      <li class="completed">
        {{todo}}
      </li>
        {{/todos}}
        {{/yetTodo}}
    </ul>
    <div class="results">

    </div>

    <!-- <script src="funk.js"></script> -->
  </body>
</html>
